{
    "name": "dicr/yii2-validate",
    "type": "yii2-extension",
    "license": "Apache-2.0",
    "description": "Validation components for Yii2",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "homepage": "https://gitlab.com/dicr/yii2-validate",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-validate.git"
    },
    "keywords": ["php", "yii2", "validate"],
    "autoload": {
        "psr-4": {
            "dicr\\validate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~8.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
